<?xml version="1.0" encoding="UTF-8"?>
<timeSeriesExportRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://chps1/schemas/timeSeriesExportRun.xsd">
	<export>
		<general>
			<exportType>SHEF</exportType>
			<folder>$EXPORT_FOLDER$/Forcings/onehr</folder>
			<exportFileName>
				<name>_HRRR_QPF_SHEFexport.xml</name>
				<prefix>
					<timeZeroFormattingString>yyyyMMddHH</timeZeroFormattingString>
				</prefix>
			</exportFileName>
			<idMapId>IdExportPIXML</idMapId> 
			<unitConversionsId>exportEnglishUnits</unitConversionsId>
			<exportMissingValue>-999</exportMissingValue>
			<omitMissingValues>true</omitMissingValues>
		</general>
   		<timeSeriesSet>
			<moduleInstanceId>Onehr_MergeMAP_Forecast</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>MAPX_R01</parameterId>
			<locationSetId>Catchments_Onehr</locationSetId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="hour" start="0" startOverrulable="false" end="30" endOverrulable="true"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>1</synchLevel>
   		</timeSeriesSet>
	</export>
</timeSeriesExportRun>