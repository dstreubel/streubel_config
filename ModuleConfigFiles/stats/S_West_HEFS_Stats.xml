	<!-- Forecast ensembles-->
	<variable>
		<variableId>Ensemble</variableId>
		<timeSeriesSet>
			<moduleInstanceSetId>ESP_QINE_Forecast_S_West</moduleInstanceSetId>
			<valueType>scalar</valueType>
			<parameterId>QINE</parameterId>
			<locationSetId>ESP_QINE_S_West</locationSetId>
			<timeSeriesType>simulated forecasting</timeSeriesType>
			<timeStep unit="hour" multiplier="6"/>
		        <relativeViewPeriod unit="hour" start="0" end="0" endOverrulable="true"/>
			<readWriteMode>read only</readWriteMode>
			<ensembleId>MEFP</ensembleId>
			<ensembleMemberIndexRange start='1971' end='2004'/>
		</timeSeriesSet>
	</variable>
	<variable>
		<variableId>ENS_Ensemble</variableId>
		<timeSeriesSet>
			<moduleInstanceSetId>ENS_SQIN_Forecast_S_West</moduleInstanceSetId>
			<valueType>scalar</valueType>
