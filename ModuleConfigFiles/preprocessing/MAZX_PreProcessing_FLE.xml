<?xml version="1.0" encoding="UTF-8"?>
<transformationModule xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://chps1/schemas/transformationModule.xsd" version="1.0">
	<variable>
		<variableId>fle_grid</variableId>
		<timeSeriesSet>
			<moduleInstanceId>ImportFLE</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>FLE</parameterId>
			<locationId>HRAP</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
			<readWriteMode>read only</readWriteMode>
		</timeSeriesSet>
	</variable>
	<variable>
		<variableId>mazx_scalar_1hr</variableId>
		<timeSeriesSet>
			<moduleInstanceId>MAZX_PreProcessing_FLE</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>MAZX</parameterId>
			<locationSetId>Catchments</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>1</synchLevel>
		</timeSeriesSet>
	</variable>
	<variable>
		<variableId>mazx_scalar_6hr</variableId>
		<timeSeriesSet>
			<moduleInstanceId>MAZX_PreProcessing_FLE</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>MAZX</parameterId>
			<locationSetId>Catchments</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="hour" multiplier="6"/>
			<relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>1</synchLevel>
		</timeSeriesSet>
	</variable>
	<variable>
		<variableId>mazx_basin_scalar_1hr</variableId>
		<timeSeriesSet>
			<moduleInstanceId>MAZX_PreProcessing_FLE</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>MAZX</parameterId>
			<locationSetId>Basins</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>1</synchLevel>
		</timeSeriesSet>
	</variable>
	<variable>
		<variableId>mazx_basin_scalar_6hr</variableId>
		<timeSeriesSet>
			<moduleInstanceId>MAZX_PreProcessing_FLE</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>MAZX</parameterId>
			<locationSetId>Basins</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="hour" multiplier="6"/>
			<relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>1</synchLevel>
		</timeSeriesSet>
	</variable>
	<transformation id="SampleMAZX">
		<interpolationSpatial>
			<average>
				<inputVariable>
					<variableId>fle_grid</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>mazx_scalar_1hr</variableId>
				</outputVariable>
			</average>
		</interpolationSpatial>
	</transformation>
	<transformation id="CHANGET_1hr_to_6hr_Scalar">
		<aggregation>
			<instantaneousToMean>
				<inputVariable>
					<variableId>mazx_scalar_1hr</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>mazx_scalar_6hr</variableId>
				</outputVariable>
			</instantaneousToMean>
		</aggregation>
	</transformation>
	<transformation id="SampleMAZXbasin">
		<interpolationSpatial>
			<average>
				<inputVariable>
					<variableId>fle_grid</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>mazx_basin_scalar_1hr</variableId>
				</outputVariable>
			</average>
		</interpolationSpatial>
	</transformation>
	<transformation id="CHANGET_1hr_to_6hr_basin_Scalar">
		<aggregation>
			<instantaneousToMean>
				<inputVariable>
					<variableId>mazx_basin_scalar_1hr</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>mazx_basin_scalar_6hr</variableId>
				</outputVariable>
			</instantaneousToMean>
		</aggregation>
	</transformation>
</transformationModule>