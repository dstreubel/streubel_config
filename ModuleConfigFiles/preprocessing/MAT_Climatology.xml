	<transformation id="MAT_Day_MY">
		<statisticsPeriodic>
			<mean>
				<inputVariable>
					<variableId>MAT_cardfiles</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MAT_Day_MY</variableId>
				</outputVariable>
			</mean>
		</statisticsPeriodic>
	</transformation>
	<transformation id="MAT_Day_MY_trimmed">
		<user>
			<simple>
				<expression>MAT_Day_MY</expression>
				<outputVariable>
					<variableId>MAT_Day_MY_trimmed</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
        <transformation id="MAT_Day_MY_Shifted">
                <timeShift>
                        <constant>
                                <inputVariable>
                                        <variableId>MAT_Day_MY</variableId>
                                </inputVariable>
                                <direction>forward</direction>
                                <numberOfTimeSteps>365</numberOfTimeSteps>
                                <outputVariable>
                                        <variableId>MAT_Day_MY_Shifted</variableId>
                                </outputVariable>
                        </constant>
                </timeShift>
        </transformation>

	<transformation id="MergeMAT">
		<merge>
			<simple>
				<inputVariable>
					<variableId>MAT_Day_MY_trimmed</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>MAT_Day_MY_Shifted</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MAT_merged_tmp</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="InterpolateMAT">
		<interpolationSerial>
			<linear>
				<inputVariable>
					<variableId>MAT_merged_tmp</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MAT_Climo</variableId>
				</outputVariable>
			</linear>
		</interpolationSerial>
	</transformation>
