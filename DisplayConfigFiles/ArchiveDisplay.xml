        <importArchiveRun>
          <archiveType>TimeSeriesArchive</archiveType>
        </importArchiveRun>
      </archiveRun>
    </archiveTask>
    <archiveTask name="Configuration archive" workflowId="Retrieve_Configuration_Archive" iconName="barrier.png" queryServiceUrl="$ARCHIVE_QUERY_SERVICE$">
