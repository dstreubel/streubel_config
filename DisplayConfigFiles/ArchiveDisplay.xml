        <importArchiveRun>
          <archiveType>ConfigurationArchive</archiveType>
        </importArchiveRun>
      </archiveRun>
    </archiveTask>
    <archiveTask name="Threshold events archive" workflowId="Retrieve_Thresholds_Archive" iconName="barrier.png" queryServiceUrl="$ARCHIVE_QUERY_SERVICE$">
